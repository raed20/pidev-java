<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.ViewAllUsersController"
            prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="usersTableView" layoutX="40.0" layoutY="50.0" prefHeight="200.0" prefWidth="550.0">
        <columns>
            <TableColumn fx:id="lastnameColumn" prefWidth="75.0" text="Last Name" />
            <TableColumn fx:id="emailColumn" prefWidth="150.0" text="Email" />
            <TableColumn fx:id="rolesColumn" prefWidth="75.0" text="Roles" />
            <TableColumn fx:id="numtelColumn" prefWidth="75.0" text="Phone Number" />
            <TableColumn fx:id="addressColumn" prefWidth="150.0" text="Address" />
            <TableColumn fx:id="otherColumn" prefWidth="100" text="Other" />


        </columns>
    </TableView>
</AnchorPane>
